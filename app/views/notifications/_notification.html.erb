<div id="<%= dom_id notification %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= notification.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Alert:</strong>
    <%= notification.alert_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Channel:</strong>
    <%= notification.channel %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Content:</strong>
    <%= notification.content %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= notification.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Read at:</strong>
    <%= notification.read_at %>
  </div>
</div>
