<div id="<%= dom_id ai_interaction_log %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= ai_interaction_log.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Session:</strong>
    <%= ai_interaction_log.session_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User query:</strong>
    <%= ai_interaction_log.user_query %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ai response:</strong>
    <%= ai_interaction_log.ai_response %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Interaction type:</strong>
    <%= ai_interaction_log.interaction_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Context entity:</strong>
    <%= ai_interaction_log.context_entity_id %>
  </div>
</div>
