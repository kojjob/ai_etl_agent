<div id="<%= dom_id audit_log %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= audit_log.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Action:</strong>
    <%= audit_log.action %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Entity:</strong>
    <%= audit_log.entity_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Old value:</strong>
    <%= audit_log.old_value %>
  </div>
  <div>
    <strong class="block font-medium mb-1">New value:</strong>
    <%= audit_log.new_value %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ip address:</strong>
    <%= audit_log.ip_address %>
  </div>
</div>
