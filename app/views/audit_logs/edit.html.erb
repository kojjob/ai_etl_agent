<% content_for :title, "Editing audit log" %>

<div class="md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing audit log</h1>

  <%= render "form", audit_log: @audit_log %>

  <%= link_to "Show this audit log", @audit_log, class: "w-full sm:w-auto text-center mt-2 sm:mt-0 sm:ml-2 rounded-md px-3.5 py-2.5 bg-gray-100 hover:bg-gray-50 inline-block font-medium" %>
  <%= link_to "Back to audit logs", audit_logs_path, class: "w-full sm:w-auto text-center mt-2 sm:mt-0 sm:ml-2 rounded-md px-3.5 py-2.5 bg-gray-100 hover:bg-gray-50 inline-block font-medium" %>
</div>
