<div id="<%= dom_id pipeline_step_run %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Pipeline run:</strong>
    <%= pipeline_step_run.pipeline_run_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Pipeline step:</strong>
    <%= pipeline_step_run.pipeline_step_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= pipeline_step_run.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Start time:</strong>
    <%= pipeline_step_run.start_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">End time:</strong>
    <%= pipeline_step_run.end_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Duration ms:</strong>
    <%= pipeline_step_run.duration_ms %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Metrics:</strong>
    <%= pipeline_step_run.metrics %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Logs summary:</strong>
    <%= pipeline_step_run.logs_summary %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Input data preview:</strong>
    <%= pipeline_step_run.input_data_preview %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Output data preview:</strong>
    <%= pipeline_step_run.output_data_preview %>
  </div>
</div>
